{
  "name": "@gerwinov/vue-project-timeline",
  "description": "Vertical timeline to display projects using vue",
  "author": "gerwinov <gerwinov@tuta.io>",
  "homepage": "https://github.com/gerwinov/vue-project-timeline",
  "version": "0.2.0",
  "license": "MIT",
  "main": "dist/VueProjectTimeline.umd.js",
  "module": "dist/VueProjectTimeline.esm.js",
  "unpkg": "dist/VueProjectTimeline.min.js",
  "browser": {
    "./sfc": "src/App.vue"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name VueProjectTimeline",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-smooth-reflow": "^0.1.6"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.0.1",
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-plugin-eslint": "^3.0.3",
    "@vue/cli-service": "^3.0.3",
    "postcss-cli": "^6.0.0",
    "postcss-import": "^12.0.0",
    "tailwindcss": "^0.6.5",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 0.1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
