{
  "name": "vue-project-timeline",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:css": "postcss ./src/assets/css/main.css -o ./public/css/styles.css",
    "watch": "watch 'npm run build:css' ./src",
    "serve": "vue-cli-service serve",
    "build": "npm run build:css && vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "animate.css": "^3.7.0",
    "vue": "^2.5.17",
    "vue-smooth-reflow": "^0.1.5"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.0.1",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "cssnano": "^4.0.5",
    "postcss-cli": "^6.0.0",
    "postcss-easy-import": "^3.0.0",
    "tailwindcss": "^0.6.5",
    "vue-template-compiler": "^2.5.17",
    "watch": "^1.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
